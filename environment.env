# Переменные окружения для системы нейросетевых инвестиций
# Скопируйте этот файл в .env и заполните реальными значениями

# DeepSeek API ключ для анализа новостей и текста
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# T-Bank (Tinkoff) Invest API токен для торговли
# Получить токен можно здесь: https://www.tbank.ru/invest/settings/api/
TINKOFF_TOKEN=your_tinkoff_token_here

# Режим песочницы T-Bank (true для тестирования, false для реальной торговли)
TINKOFF_SANDBOX=true

# ID счета T-Bank (опционально, для sandbox создастся автоматически)
TINKOFF_ACCOUNT_ID=

# Дополнительные настройки
LOG_LEVEL=INFO
DEBUG_MODE=false

# Настройки для разработки
DEVELOPMENT_MODE=true
